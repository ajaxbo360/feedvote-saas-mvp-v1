{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "npm install --no-frozen-lockfile && npm install @radix-ui/react-avatar @radix-ui/react-progress @radix-ui/react-switch @radix-ui/react-tooltip"
      }
    }
  ],
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "devCommand": "npm run dev",
  "ignoreCommand": "if [[ \"$VERCEL_GIT_COMMIT_REF\" == \"staging\" ]]; then exit 0; else exit 1; fi"
}
